.carousel{height:280rpx;position:relative;overflow:hidden;transform:translateY(0);background-color:#efefef}.carousel .indicator{position:absolute;left:0;right:0;bottom:16rpx;display:flex;justify-content:center}.carousel .indicator .dot{width:30rpx;height:6rpx;margin:0 8rpx;border-radius:6rpx;background-color:rgba(255,255,255,.4)}.carousel .indicator .active{background-color:#fff}.carousel .navigator,.carousel .image{width:100%;height:100%}
